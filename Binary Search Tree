class Node:
    def __init__(self, key):
        self.key = key
        self.left = None
        self.right = None
 def insert(root, key):
    if not root:
        return Node(key)
    if key < root.key:
        root.left = insert(root.left, key)
    else:
        root.right = insert(root.right, key)
    return root
 def inorder(root):
    if root:
        inorder(root.left)
        print(root.key, end=" ")
        inorder(root.right)
 def search(root, key):
    if not root or root.key == key:
        return root
    if key < root.key:
        return search(root.left, key)
    return search(root.right, key)
 def exp9():
    root = None
    while True:
        print("\n--- Experiment 9: BST ---")
        print("1. Insert")
        print("2. Display (Inorder)")
        print("3. Search")
        print("4. Exit")
        ch = int(input("Enter choice: "))
        if ch == 1:
            val = int(input("Enter value: "))
            root = insert(root, val)
        elif ch == 2:
            inorder(root); print()
        elif ch == 3:
            val = int(input("Enter value: "))
            print("Found" if search(root, val) else "Not Found")
        elif ch == 4: break
 if __name__ == "__main__":
    exp9()

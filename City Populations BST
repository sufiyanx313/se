class Node:
    def __init__(self, city, pop):
        self.city = city
        self.pop = pop
        self.left = None
        self.right = None
 def insert(root, city, pop):
    if not root:
        return Node(city, pop)
    if city < root.city:
        root.left = insert(root.left, city, pop)
    else:
        root.right = insert(root.right, city, pop)
    return root
 def inorder(root):
    if root:
        inorder(root.left)
        print(root.city, "-", root.pop)
        inorder(root.right)
 def search(root, city):
    if not root or root.city == city:
        return root
    if city < root.city:
        return search(root.left, city)
    return search(root.right, city)
 def exp10():
    root = None
    while True:
        print("\n--- Experiment 10: City Population BST ---")
        print("1. Insert City")
        print("2. Display (Inorder)")
        print("3. Search City")
        print("4. Exit")
        ch = int(input("Enter choice: "))
        if ch == 1:
            c = input("Enter city: "); p = int(input("Enter population: "))
            root = insert(root, c, p)
        elif ch == 2:
            inorder(root)
        elif ch == 3:
            c = input("Enter city to search: ")
            res = search(root, c)
            print("Found:", res.pop) if res else print("Not Found")
        elif ch == 4: break
 if __name__ == "__main__":
    exp10()
    return search(root.right, city)

def selection_sort(arr):
    n = len(arr)
    for i in range(n):
        min_idx = i
        for j in range(i+1, n):
            if arr[j] < arr[min_idx]:
                min_idx = j
        arr[i], arr[min_idx] = arr[min_idx], arr[i]
 def linear_search(arr, x):
    for i in range(len(arr)):
        if arr[i] == x:
            return i
    return -1
 def binary_search(arr, x):
    low, high = 0, len(arr)-1
    while low <= high:
        mid = (low + high) // 2
        if arr[mid] == x:
            return mid
        elif arr[mid] < x:
            low = mid + 1
        else:
            high = mid - 1
    return -1
 def main():
    arr = []
    while True:
        print("\n--- Experiment 1: Search ---")
        print("1. Enter IDs")
        print("2. Linear Search")
        print("3. Binary Search")
        print("4. Exit")
        ch = int(input("Enter choice: "))
        if ch == 1:
            n = int(input("How many IDs? "))
            arr = []
            for i in range(n):
                arr.append(int(input(f"Enter ID {i+1}: ")))
            selection_sort(arr)
        elif ch == 2:
            x = int(input("Enter ID to search: "))
            pos = linear_search(arr, x)
            print("Found" if pos != -1 else "Not Found")
        elif ch == 3:
            x = int(input("Enter ID to search: "))
            pos = binary_search(arr, x)
            print("Found" if pos != -1 else "Not Found")
        elif ch == 4:
            break
 if __name__ == "__main__":
    main()

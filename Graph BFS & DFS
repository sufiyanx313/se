from collections import deque
 def bfs(graph, start):
    visited = set([start])
    queue = deque([start])
    while queue:
        v = queue.popleft()
        print(v, end=" ")
        for neigh in graph[v]:
            if neigh not in visited:
                visited.add(neigh)
                queue.append(neigh)
 def dfs(graph, start, visited=None):
    if visited is None:
        visited = set()
    visited.add(start)
    print(start, end=" ")
    for neigh in graph[start]:
        if neigh not in visited:
            dfs(graph, neigh, visited)
 def exp8():
    graph = {}
    n = int(input("Enter number of vertices: "))
    for i in range(n):
        v = input(f"Enter vertex {i+1}: ")
        graph[v] = input(f"Enter neighbors of {v} (space separated): ").split()
    while True:
        print("\n--- Experiment 8: Graph Traversal ---")
        print("1. BFS")
        print("2. DFS")
        print("3. Exit")
        ch = int(input("Enter choice: "))
        if ch == 1:
            start = input("Enter start vertex: ")
            bfs(graph, start); print()
        elif ch == 2:
            start = input("Enter start vertex: ")
            dfs(graph, start); print()
        elif ch == 3: break
 if __name__ == "__main__":
    exp8()
